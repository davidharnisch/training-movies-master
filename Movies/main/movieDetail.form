            </Include>
          </Children>
        </Complex>
        <Attribute>
          <Name value="id" />
          <Caption value="Id" />
          <DataContextParameter.IsInput value="True" />
          <DeclaredType>
            <AttributeType />
          </DeclaredType>
        </Attribute>
        <Attribute>
          <Name value="type" />
          <Caption value="Type" />
          <MetaType reference="+MetaTypes.Type" />
          <OnChange>
            <ActionEvent ReferencedAction="+UiParts.movieDetail.handler_type_OnChange" />
          </OnChange>
        </Attribute>
